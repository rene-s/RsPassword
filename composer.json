{
  "name": "reneschmidt/rspassword",
  "description": "RsPassword is a small PHP class for generating salted password hashes with rounds and for verifying salted password hashes with rounds.",
  "minimum-stability": "stable",
  "license": "LGPL-3.0",
  "authors": [
    {
      "name": "Rene Schmidt",
      "email": "rene+_pkg@reneschmidt.de"
    }
  ],
  "require-dev": {
    "phpunit/phpunit": "3.7.x",
    "phpunit/php-code-coverage": "1.2.x",
    "squizlabs/php_codesniffer": "1.4.8"
  },
  "autoload": {
    "psr-4": {
      "RsPassword\\": "src/RsPassword/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "RsPassword\\": "src/RsPassword/"
    }
  }
}