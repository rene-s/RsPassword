{
  "name": "reneschmidt/rspassword",
  "description": "RsPassword is a small PHP class for generating salted password hashes with rounds and for verifying salted password hashes with rounds.",
  "minimum-stability": "stable",
  "license": "LGPL-3.0",
  "authors": [
    {
      "name": "Rene Schmidt",
      "email": "rene+_pkg@reneschmidt.de"
    }
  ],
  "require-dev": {
    "phpunit/phpunit": "4.5.*",
    "phpunit/php-code-coverage": "2.*",
    "squizlabs/php_codesniffer": "2.*"
  },
  "autoload": {
    "psr-4": {
      "RsPassword\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "RsPassword\\": "src/",
      "RsPassword\\Tests\\": "test/unit"
    }
  }
}
